<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
>
    <Dialog
        id="ImportUserDialog"
        title="Importação de usuários"
    >
        <content>
        <VBox class="sapMPageBgStandard ">
            <VBox justifyContent="Center" class="sapUiSmallMargin sap-xp-rounding"  alignItems="Center" backgroundDesign="Solid">
                <HBox direction="Column" class="sapUiMediumMargin" alignItems="Center" justifyContent="Center">
                <Text class="sapUiTinyMarginEnd" text="Carregamento concluído"></Text>
                <ProgressIndicator
                    class="sapUiSmallMarginBottom"
                    width="20rem"
                    percentValue="100"
                    displayValue="100%"
                    showValue="true"
                    state="Success" />
                </HBox>          
            </VBox>
        </VBox>
        </content>
        <footer>
			<Toolbar>
				<content>
					<ToolbarSpacer />
                    <Button
						text="Ok"
						type="Emphasized"
						press=".onCloseDialog"
						ariaHasPopup="Dialog" />
				</content>
			</Toolbar>
		</footer>
    </Dialog>
</core:FragmentDefinition>