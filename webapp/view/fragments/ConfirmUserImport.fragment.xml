<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
>
    <Dialog
        id="importUsersDialog"
        title="Importação de usuários"
    >
        <content>
        <VBox class="sapMPageBgStandard ">
            <VBox justifyContent="Center" class="sapUiSmallMargin sap-xp-rounding"  alignItems="Center" backgroundDesign="Solid">
                <HBox direction="Column" class="sapUiMediumMargin" alignItems="Center" justifyContent="Center">
                <Text class="sapUiTinyMarginEnd sap-xp-labelBaseFontSize" text="Carregamento ..."></Text>
                <ProgressIndicator
                    class="sapUiSmallMarginBottom"
                    width="20rem"
                    percentValue="30"
                    displayValue="30%"
                    showValue="true"
                    state="Success" />
                </HBox>          
            </VBox>
        </VBox>
        </content>
        <footer>
			<Toolbar>
				<content>
					<ToolbarSpacer />
                    <Button id="ImportUserDialog"
						text="Ok"
						type="Default"
						press=".oncloseDialog"
						ariaHasPopup="Dialog" />
				</content>
			</Toolbar>
		</footer>
    </Dialog>
</core:FragmentDefinition>